{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "edgecalidraw",
  "main": "apps/excalidraw-backend/src/index.ts",
  "compatibility_date": "2025-03-21",
  "compatibility_flags": [
    "nodejs_compat_v2"
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": "apps/excalidraw-frontend/dist",
    "not_found_handling": "single-page-application",
    "run_worker_first": [
      "/api/*"
    ]
  },
  "observability": {
    "enabled": true,
  },
  "durable_objects": {
    "bindings": [
      {
        "class_name": "ExcalidrawWebSocketServer",
        "name": "DURABLE_OBJECT",
      },
    ],
  },
  "migrations": [
    {
      "tag": "v1",
      "new_classes": [
        "ExcalidrawWebSocketServer"
      ]
    }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "edgecalidraw-db",
      "database_id": "8624e44c-0706-4935-8658-8ae17d1c488a"
    }
  ]
}